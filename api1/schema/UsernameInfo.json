{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "UsernameInfo",
    "type": "object",
    "properties": {
        "createdAt": {
            "type": "string",
            "format": "date-time"
        },
        "lastSyncAt": {
            "type": "string",
            "format": "date-time"
        },
        "lastLoginAt": {
            "type": "string",
            "format": "date-time"
        },  
        "username": {
            "type": "string",
            "minLength": 3,
            "maxLength": 64
        },
        "displayName": {
            "type": "string",
            "minLength": 3,
            "maxLength": 128
        },
        "phone": {
            "type": "string",
            "pattern": "^[+]?[(]?[0-9]{1,4}[)]?[-s./0-9]*$"
        },
        "email": {
            "type": "string",
            "format": "email",
            "maxLength": 256
        }   
    },
    "additionalProperties": true
}