{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "UsernamePasswordLogin",
    "type": "object",
    "properties": {
        "username": {
            "type": "string",
            "minLength": 3,
            "maxLength": 64
        },
        "password": {
            "type": "string",
            "minLength": 8,
            "maxLength": 128
        }
    },
    "required": ["username", "password"],
    "additionalProperties": false
}